<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sairamkrishna.intelimall" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logomall"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logomall"
        android:supportsRtl="true"
        android:theme="@style/Theme.Intelimall" >
        <activity
            android:name=".CartItems"
            android:exported="true" />
        <activity android:name=".MainCategories" />
        <activity
            android:name=".HomeDrawer"
            android:label="@string/title_activity_home_drawer"
            android:theme="@style/Theme.Intelimall" />
        <activity android:name=".RegistrationEctivity" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>